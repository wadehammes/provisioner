{
  "trailingComma": "all",
  "tabWidth": 2,
  "semi": true,
  "plugins": ["prettier-plugin-organize-imports"],
  "overrides": [
    {
      "files": "*.{ts, tsx}",
      "options": {
        "parser": "babel-ts"
      }
    },
    {
      "files": ["*.css"],
      "options": {
        "parser": "css"
      }
    }
  ]
}

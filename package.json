{
  "name": "next-provisioner",
  "version": "0.0.0",
  "packageManager": "pnpm@10.7.0",
  "private": true,
  "scripts": {
    "dev": "NODE_OPTIONS='--inspect' next dev -p 7777",
    "build": "next build && make sitemap",
    "start": "next start -p 7777",
    "export": "next export",
    "biome:fix": "biome format --write .",
    "tsc:ci": "tsc --strict",
    "lint:ci": "biome ci --reporter=github",
    "lint:fix": "biome check --fix .",
    "lint:css": "stylelint ./src/**/*.css",
    "test:ci": "jest --passWithNoTests",
    "types:contentful": "export $(cat .env.local | sed '/^#/d; s/[$\"]//g'); cf-content-types-generator -s $CONTENTFUL_SPACE_ID -t $CONTENTFUL_CMA_TOKEN -o src/contentful/types -X && biome format --write src/contentful/types"
  },
  "dependencies": {
    "@contentful/rich-text-react-renderer": "^16.0.1",
    "@contentful/rich-text-types": "^17.0.0",
    "@faker-js/faker": "^9.6.0",
    "@jest/types": "^29.6.3",
    "@next/third-parties": "^15.2.4",
    "@svgr/webpack": "^8.1.0",
    "@tanstack/react-query": "^5.71.5",
    "classnames": "^2.5.1",
    "contentful": "^11.5.13",
    "gsap": "^3.12.7",
    "html-react-parser": "^5.2.3",
    "jest-mock": "^29.7.0",
    "next": "^15.2.4",
    "postcss-flexbugs-fixes": "^5.0.2",
    "postcss-preset-env": "^10.1.5",
    "postcss-syntax": "^0.36.2",
    "react": "^19.1.0",
    "react-aria": "^3.38.1",
    "react-dom": "^19.1.0",
    "react-google-recaptcha": "^3.1.0",
    "react-hook-form": "^7.55.0",
    "react-intersection-observer": "^9.16.0",
    "react-player": "^2.16.0",
    "react-toastify": "^11.0.5",
    "resend": "4.2.0",
    "schema-dts": "^1.1.5",
    "stylelint-webpack-plugin": "^5.0.1",
    "swiper": "^11.2.6",
    "tslib": "^2.8.1",
    "usehooks-ts": "^3.1.1"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.14.0",
    "@types/react": "^19.1.0",
    "@types/react-google-recaptcha": "^2.1.9",
    "cf-content-types-generator": "^2.15.5",
    "csstype": "^3.1.3",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "react-is": "^19.1.0",
    "stylelint": "^16.17.0",
    "stylelint-config-css-modules": "^4.4.0",
    "stylelint-config-recommended": "^15.0.0",
    "stylelint-prettier": "^5.0.3",
    "ts-jest": "^29.3.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.2",
    "typescript-plugin-css-modules": "^5.1.0"
  }
}

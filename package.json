{
  "name": "next-provisioner",
  "version": "0.0.0",
  "packageManager": "pnpm@9.12.0",
  "private": true,
  "scripts": {
    "dev": "NODE_OPTIONS='--inspect' next dev -p 7777",
    "build": "next build && make sitemap",
    "start": "next start -p 7777",
    "export": "next export",
    "biome:fix": "biome format --write .",
    "tsc:ci": "tsc --strict",
    "lint:ci": "biome lint .",
    "lint:fix": "biome lint --apply .",
    "lint:css": "stylelint ./src/**/*.css",
    "test:ci": "jest --passWithNoTests",
    "types:contentful": "export $(cat .env.local | sed '/^#/d; s/[$\"]//g'); cf-content-types-generator -s $CONTENTFUL_SPACE_ID -t $CONTENTFUL_CMA_TOKEN -o src/contentful/types -X && biome format --write src/contentful/types"
  },
  "dependencies": {
    "@contentful/rich-text-react-renderer": "^15.22.11",
    "@contentful/rich-text-types": "^16.8.5",
    "@faker-js/faker": "^9.0.3",
    "@jest/types": "^29.6.3",
    "@next/third-parties": "^14.2.14",
    "@notionhq/client": "^2.2.15",
    "@svgr/webpack": "^8.1.0",
    "@tanstack/react-query": "^5.59.0",
    "classnames": "^2.5.1",
    "contentful": "^11.0.3",
    "gsap": "^3.12.5",
    "html-react-parser": "^5.1.16",
    "jest-mock": "^29.7.0",
    "next": "^14.2.14",
    "postcss-flexbugs-fixes": "^5.0.2",
    "postcss-preset-env": "^10.0.5",
    "postcss-syntax": "^0.36.2",
    "react": "^18.3.1",
    "react-aria": "^3.35.0",
    "react-dom": "^18.3.1",
    "react-google-recaptcha": "^3.1.0",
    "react-hook-form": "^7.53.0",
    "react-intersection-observer": "^9.13.1",
    "react-toastify": "^10.0.5",
    "resend": "^4.0.0",
    "schema-dts": "^1.1.2",
    "stylelint-webpack-plugin": "^5.0.1",
    "swiper": "^11.1.14",
    "tslib": "^2.7.0",
    "usehooks-ts": "^3.1.0"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.3",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.5.0",
    "@testing-library/react": "^16.0.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/jest": "^29.5.13",
    "@types/node": "^22.7.4",
    "@types/react": "^18.3.11",
    "@types/react-google-recaptcha": "^2.1.9",
    "cf-content-types-generator": "^2.15.5",
    "csstype": "^3.1.3",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "react-is": "^18.3.1",
    "stylelint": "^16.9.0",
    "stylelint-config-css-modules": "^4.4.0",
    "stylelint-config-recommended": "^14.0.1",
    "stylelint-prettier": "^5.0.2",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.2",
    "typescript-plugin-css-modules": "^5.1.0"
  }
}

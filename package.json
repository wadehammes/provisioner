{
  "name": "next-provisioner",
  "version": "0.0.0",
  "packageManager": "pnpm@10.23.0",
  "private": true,
  "scripts": {
    "dev": "NODE_OPTIONS='--inspect' next dev -p 7777",
    "build": "next build && make sitemap",
    "start": "next start -p 7777",
    "export": "next export",
    "biome:fix": "biome format --write .",
    "tsc:ci": "tsc --strict",
    "lint:ci": "biome ci --reporter=github",
    "lint:fix": "biome check --fix .",
    "test:ci": "jest --passWithNoTests",
    "types:contentful": "export $(cat .env.local | sed '/^#/d; s/[$\"]//g'); cf-content-types-generator -s $CONTENTFUL_SPACE_ID -t $CONTENTFUL_CMA_TOKEN -o src/contentful/types -X && biome format --write src/contentful/types"
  },
  "dependencies": {
    "@contentful/rich-text-react-renderer": "^16.1.6",
    "@contentful/rich-text-types": "^17.2.5",
    "@faker-js/faker": "^10.1.0",
    "@jest/types": "^30.2.0",
    "@next/third-parties": "^16.0.4",
    "@svgr/webpack": "^8.1.0",
    "@tanstack/react-query": "^5.90.10",
    "classnames": "^2.5.1",
    "contentful": "^11.9.0",
    "gsap": "^3.13.0",
    "html-react-parser": "^5.2.10",
    "jest-mock": "^30.2.0",
    "next": "^16.0.4",
    "postcss-flexbugs-fixes": "^5.0.2",
    "postcss-preset-env": "^10.4.0",
    "postcss-syntax": "^0.36.2",
    "react": "^19.2.0",
    "react-aria": "^3.44.0",
    "react-dom": "^19.2.0",
    "react-google-recaptcha": "^3.1.0",
    "react-hook-form": "^7.66.1",
    "react-intersection-observer": "^10.0.0",
    "react-player": "^3.4.0",
    "react-toastify": "^11.0.5",
    "resend": "6.5.2",
    "schema-dts": "^1.1.5",
    "swiper": "^12.0.3",
    "tslib": "^2.8.1",
    "usehooks-ts": "^3.1.1"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.7",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.7",
    "@types/react-google-recaptcha": "^2.1.9",
    "cf-content-types-generator": "^2.16.1",
    "csstype": "^3.2.3",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "react-is": "^19.2.0",
    "ts-jest": "^29.4.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3",
    "typescript-plugin-css-modules": "^5.2.0"
  }
}
